{
    "manifest_version": 2,

    "name": "my ext",
    "description": "my ext desc",
    "version": "1.0",

    "browser_action": {
        "default_icon": "mouse.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://*.pinterest.com/*"],
        "css": ["content.css"],
        "js": ["jquery-3.3.1.min.js", "content.js"]
    }],
    "permissions": [
        "activeTab",
        "storage"
    ]
}